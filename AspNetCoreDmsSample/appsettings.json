{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System" : "Information",
      "Microsoft" : "Information"
    }
  },
  "AppSettings" : {
    "SQLConnectionString" : "User ID={user id};Password={password};Initial Catalog={database};Data Source={server};", 
    "MySQLConnectionString" : "User ID={user id};Password={password};Database={database};Server={server};"
  },
  // As the EC2 instance has role, you don't need to fix credentials on appsettings.json
  "AWS" : {
    "Profile" : "default",
    "Region" : "us-east-1"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:5002"
      }
    }
  }
}
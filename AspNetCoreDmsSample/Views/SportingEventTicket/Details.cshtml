@model DMSSample.Models.SportingEventTicket

@{
    Layout = "~/Views/Shared/_FormLayout.cshtml";
    ViewData["Title"] = "Details";
}
<div class="card card-register mx-auto mt-5">
    <div class="card-header">Ticket Details</div>
    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.TicketPrice)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.TicketPrice)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.S)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.S.SeatSection)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.SportingEvent)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.SportingEvent.SportTypeName)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.Ticketholder)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.Ticketholder.FullName)
            </dd>
        </dl>
    </div>
    <div class="text-center">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-info">Edit</a>
        <a asp-action="Index" class="btn btn-secondary">Back To List</a>
    </div>
    <p></p>
</div>